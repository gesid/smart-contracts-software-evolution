contract ownable {
  address public owner;

  function ownable() {
    owner = msg.sender;
  }

  modifier onlyowner() {
    if (msg.sender != owner) {
      throw;
    }
    _;
  }

  function transferownership(address newowner) onlyowner {
    if (newowner != address(0)) {
      owner = newowner;
    }
  }

}

