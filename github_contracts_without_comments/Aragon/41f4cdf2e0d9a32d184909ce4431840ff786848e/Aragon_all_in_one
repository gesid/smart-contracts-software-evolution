contract ant is minimeirrevocablevestedtoken {
  
  function ant(
    address _tokenfactory
  ) minimeirrevocablevestedtoken(
    _tokenfactory,
    0x0,                    
    0,                      
    , 
    18,                     
    ,                  
    true                    
    ) {}
}

pragma solidity ^0.4.8;

