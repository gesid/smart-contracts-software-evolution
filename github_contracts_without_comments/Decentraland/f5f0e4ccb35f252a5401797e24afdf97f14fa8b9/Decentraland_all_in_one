  function updatelanddata(int x, int y, string data) external;
  function updatemanylanddata(int[] x, int[] y, string data) external;

  

  event update(  
    uint256 indexed assetid, 
    address indexed holder,  
    address indexed operator,  
    string data  
  );
}



